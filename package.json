{
  "name": "unuse-ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.5",
  "license": "MIT",
  "repository": "https://github.com/jbaubree/unuse-ui",
  "exports": {
    ".": {
      "require": "./dist/unuse-ui.umd.cjs",
      "import": "./dist/unuse-ui.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/unuse-ui.umd.cjs",
  "module": "./dist/unuse-ui.js",
  "types": "./dist/unuse-ui.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:playground": "cd playground && pnpm dev",
    "dev:docs": "vitepress dev docs",
    "dev:lib": "vite build --watch",
    "build": "npm-run-all build:*",
    "build:lib": "vite build",
    "build:docs": "vitepress build docs",
    "lint": "eslint . --max-warnings 0",
    "postinstall": "npx simple-git-hooks",
    "release": "bumpp --commit --push --tag",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -r -I"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@iconify-json/ph": "^1.1.5",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.2.0",
    "@vueuse/head": "^1.1.26",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.2",
    "@unocss/eslint-config": "^0.53.4",
    "@unocss/preset-web-fonts": "^0.53.4",
    "@unocss/reset": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "bumpp": "^9.1.1",
    "critters": "^0.0.19",
    "eslint": "^8.43.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.5",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitepress": "1.0.0-beta.3",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.8.2"
  }
}
