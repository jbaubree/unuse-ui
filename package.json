{
  "name": "unuse-ui",
  "type": "module",
  "version": "0.0.84",
  "packageManager": "pnpm@8.6.12",
  "license": "MIT",
  "repository": "https://github.com/jbaubree/unuse-ui",
  "exports": {
    ".": {
      "import": "./dist/unuse-ui.js",
      "require": "./dist/unuse-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/unuse-ui.umd.cjs",
  "module": "./dist/unuse-ui.js",
  "types": "./dist/unuse-ui.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:playground": "cd playground && pnpm dev",
    "dev:docs": "vitepress dev docs",
    "dev:lib": "vite build --watch",
    "build": "npm-run-all build:*",
    "build:lib": "vite build",
    "build:docs": "vitepress build docs",
    "lint": "eslint . --max-warnings 0",
    "release": "bumpp --commit --push --tag",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -r -I"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@iconify-json/ph": "^1.1.6",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^1.3.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.5.7",
    "@unocss/eslint-config": "^0.55.3",
    "@unocss/reset": "^0.55.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "bumpp": "^9.2.0",
    "critters": "^0.0.20",
    "eslint": "^8.48.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.7.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "unocss": "^0.55.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitepress": "1.0.0-rc.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  }
}
