{
  "name": "unuse-ui",
  "type": "module",
  "version": "0.1.16",
  "packageManager": "pnpm@8.12.1",
  "license": "MIT",
  "repository": "https://github.com/jbaubree/unuse-ui",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/unuse-ui.js",
      "require": "./dist/unuse-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/unuse-ui.umd.cjs",
  "module": "./dist/unuse-ui.js",
  "types": "./dist/unuse-ui.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:playground": "cd playground && pnpm dev",
    "dev:docs": "vitepress dev docs",
    "dev:lib": "vite build --watch",
    "build": "npm-run-all build:*",
    "build:lib": "vite build",
    "build:docs": "vitepress build docs",
    "lint": "eslint . --max-warnings 0",
    "release": "bumpp --commit --push --tag",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -r -I"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@iconify-json/ph": "^1.1.9",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.7.0",
    "@vueuse/head": "^2.0.0",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.4",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/reset": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "critters": "^0.0.20",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.12.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vitepress": "1.0.0-rc.32",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.26"
  }
}
