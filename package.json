{
  "name": "unuse-ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.1",
  "license": "MIT",
  "repository": "https://github.com/jbaubree/unuse-ui",
  "exports": {
    ".": {
      "require": "./dist/unuse-ui.umd.cjs",
      "import": "./dist/unuse-ui.js"
    }
  },
  "main": "./dist/unuse-ui.umd.cjs",
  "module": "./dist/unuse-ui.js",
  "types": "./dist/unuse-ui.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:docs": "vitepress dev docs",
    "dev:lib": "vite build --watch",
    "build": "npm-run-all build:*",
    "build:lib": "vite build",
    "build:docs": "vitepress build docs",
    "lint": "eslint . --max-warnings 0",
    "postinstall": "npx simple-git-hooks",
    "release": "bumpp --commit --push --tag",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -r -I"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "defu": "^6.1.2",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@iconify-json/ph": "^1.1.5",
    "@unocss/eslint-config": "^0.53.1",
    "@unocss/preset-web-fonts": "^0.53.1",
    "@unocss/reset": "^0.53.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "bumpp": "^9.1.1",
    "critters": "^0.0.18",
    "eslint": "^8.42.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.1",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "unplugin-auto-import": "^0.16.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitepress": "1.0.0-beta.2",
    "vue-tsc": "^1.6.5"
  }
}
